@model IEnumerable<TechPractics2.Models.EDM.UserToCustomer>

@{
    ViewBag.Title = "UserToCustomerCollection";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UserToCustomerCollection</h2>

<p>
    @Html.ActionLink("Create New", "Add")
</p>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.User)</th>
        <th>@Html.DisplayNameFor(model => model.Customer)</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.User.FullDisc)</td>
        <td>@Html.DisplayFor(modelItem => item.Customer.FullDisc)</td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}

</table>
